{
  "version": "2.0",
  "functionTimeout": "00:05:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "signalRService": {
      "connectionStringSetting": "AzureSignalRConnectionString"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information"
    }
  },
  "http": {
    "cors": {
      "allowedOrigins": [
        "http://localhost:5173",
        "http://127.0.0.1:5173",
        "http://localhost:5174",
        "http://127.0.0.1:5174"
      ],
      "allowedMethods": [
        "GET",
        "POST",
        "OPTIONS"
      ],
      "allowedHeaders": [
        "Content-Type",
        "Authorization",
        "x-signalr-user-agent",
        "x-requested-with"
      ],
      "supportCredentials": false
    }
  }
}
